// version.rc.in
#define VER_FILEVERSION         @PRODUCT_VERSION_MAJOR@,@PRODUCT_VERSION_MINOR@,@PRODUCT_VERSION_PATCH@,@PRODUCT_VERSION_BUILD_NUMBER@
#define VER_FILEVERSION_STR    "@PRODUCT_VERSION_MAJOR@.@PRODUCT_VERSION_MINOR@.@PRODUCT_VERSION_PATCH@.@PRODUCT_VERSION_BUILD_NUMBER@\0"

#define VER_PRODUCTVERSION      @PRODUCT_VERSION_MAJOR@,@PRODUCT_VERSION_MINOR@,@PRODUCT_VERSION_PATCH@,@PRODUCT_VERSION_BUILD_NUMBER@
#define VER_PRODUCTVERSION_STR "@PRODUCT_VERSION_MAJOR@.@PRODUCT_VERSION_MINOR@.@PRODUCT_VERSION_PATCH@.@PRODUCT_VERSION_BUILD_NUMBER@\0"

#define VER_COMPANYNAME_STR "@COMPANY_NAME@\0"
#define VER_FILEDESCRIPTION_STR "@FILE_DESCRIPTION@\0"
#define VER_INTERNALNAME_STR "@INTERNAL_NAME@\0"
#define VER_LEGALCOPYRIGHT_STR "@LEGAL_COPYRIGHT@\0"
#define VER_ORIGINALFILENAME_STR "@ORIGINAL_FILENAME@\0"
#define VER_PRODUCTNAME_STR "@PRODUCT_NAME@\0"

//

1 VERSIONINFO
FILEVERSION VER_FILEVERSION
PRODUCTVERSION VER_PRODUCTVERSION
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
      VALUE "CompanyName", VER_COMPANYNAME_STR
      VALUE "FileDescription", VER_FILEDESCRIPTION_STR
      VALUE "InternalName", VER_INTERNALNAME_STR
      VALUE "LegalCopyright", VER_LEGALCOPYRIGHT_STR
      VALUE "OriginalFilename", VER_ORIGINALFILENAME_STR
      VALUE "ProductName", VER_PRODUCTNAME_STR
      VALUE "FileVersion", VER_FILEVERSION_STR
      VALUE "ProductVersion", VER_PRODUCTVERSION_STR
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1252
  END
END